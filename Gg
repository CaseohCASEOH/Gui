local OrionLib = loadstring(game:HttpGet(('https://pastebin.com/raw/WRUyYTdY')))()
local Window = OrionLib:MakeWindow({Name = "Natural Disaster Survival V1", HidePremium = false, SaveConfig = true, ConfigFolder = "Orion"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
OrionLib:MakeNotification({
	Name = "THX FOR USING OUR SCRIPT!",
	Content = "Join Discord server, there will be script news and more",
	Image = "rbxassetid://4483345998",
	Time = 31
})

local timeLabel = Tab:AddLabel("Real-life time will appear here")

local function updateTime()
	while true do
		local realTime = os.date("!%Y-%m-%d %H:%M:%S")
		local ms = tick() % 1 * 1000
		local timeString = string.format("%s.%03d", realTime, math.floor(ms))
		timeLabel:Set(timeString)
		wait(0.1)
	end
end

spawn(updateTime)

Tab:AddLabel("Made By aabbaaii")

