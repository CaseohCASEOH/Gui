local Tab = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
    Name = "Current Disaster",
    Callback = function()
        local survivalTag        
        for _, obj in pairs(workspace:GetChildren()) do
            if obj:FindFirstChild("SurvivalTag") then
                survivalTag = obj.SurvivalTag.Value
                break
            end
        end

        if survivalTag then
            OrionLib:MakeNotification({
                Name = "CURRENT DISASTER",
                Content = "DISASTER: " .. survivalTag,
                Image = "rbxassetid://4483345998",
                Time = 15
            })
        else
            OrionLib:MakeNotification({
                Name = "CURRENT DISASTER",
                Content = "No disaster found",
                Image = "rbxassetid://4483345998",
            })
        print("working")
    end
})

Tab:AddButton({
	Name = "Infinite yield",
	Callback = function(value)
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
      	print("inf yield executed")
  	end
})

Tab:AddButton({
	Name = "Leave game",
	Callback = function(value)
game.Players.LocalPlayer:Kick("you just left the game!")
      	print("kicked")
  	end
})

Tab:AddToggle({
    Name = "Anti Fling (test)",
    Default = false,
    Callback = function(value)
        antiFlingEnabled = value
        if antiFlingEnabled then
            print("Anti fling activated")
        else
            print("Anti fling deactivated")
        end
    end
})

game:GetService("RunService").Stepped:Connect(function()
    local character = game.Players.LocalPlayer.Character
    if character and antiFlingEnabled then
        character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        character.HumanoidRootPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
    end
end)

Tab:AddButton({
    Name = "Reset",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player.Character then
            player.Character:BreakJoints()
        end
        print("Character reseted")
    end
})

OrionLib:Init()
